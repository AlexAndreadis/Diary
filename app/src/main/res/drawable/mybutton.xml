<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#86592d" />
            <gradient android:angle="-90" android:startColor="#ac7339" android:endColor="#86592d"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#86592d" />
            <solid android:color="#c68c53"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#86592d" />
            <gradient android:angle="-90" android:startColor="#bf8040" android:endColor="#604020" />
        </shape>
    </item>
</selector>